{
  "name": "tizen",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18 <20"
  },
  "scripts": {
    "preinstall": "node -e \"process.exit(0)\"",
    "dev": "vite",
    "build": "node scripts/esbuild-build.mjs",
    "lint": "eslint .",
    "preview": "vite preview --port 3000 --strictPort",
    "build:tizen": "node scripts/esbuild-build.mjs",
    "package:tizen": "cd dist && zip -r ../app.wgt * ../config.xml"
  },
  "dependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "4.3.4",
    "esbuild": "0.21.5",
    "eslint": "9.36.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.22",
    "globals": "16.4.0",
    "vite": "5.4.11"
  },
  "browserslist": [
    "defaults",
    "not ie <= 11",
    "maintained node versions"
  ]
}
